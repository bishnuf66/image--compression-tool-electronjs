{
  "name": "webp-converter",
  "version": "1.0.0",
  "description": "Desktop WebP Image Converter with Size Optimization",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "webp",
    "image",
    "converter",
    "optimization"
  ],
  "author": "Bishnu Bishowkarma",
  "license": "MIT",
  "devDependencies": {
    "electron": "37.0.0",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "detect-libc": "^2.0.4",
    "electron-squirrel-startup": "^1.0.1",
    "semver": "^7.7.2",
    "sharp": "^0.34.3"
  },
  "build": {
    "appId": "com.bishnu.webp-converter",
    "productName": "WebP Converter By Bishnu",
    "icon": "assets/icon.png",
    "mac": {
      "icon": "assets/icon.png"
    },
    "win": {
      "target": ["nsis"],
      "icon": "assets/icon.png"
    },
    "linux": {
      "target": ["AppImage", "deb"],
      "icon": "assets/icon.png"
    }
  }
}
